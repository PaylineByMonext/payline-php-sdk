{
    "name": "monext/payline-sdk",
    "description": "Payline library for PHP",
    "keywords": [
        "Monext",
        "Payline",
        "payment"
    ],
    "type": "library",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Fabien SUAREZ",
            "email": "fabien.suarez@payline.com",
            "homepage": "http://www.payline.com"
        },
        {
            "name": "Vincent Pietri",
            "email": "vincent.pietri@tbdgroup.com",
            "homepage": "https://www.tbdgroup.com/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-soap": "*",
